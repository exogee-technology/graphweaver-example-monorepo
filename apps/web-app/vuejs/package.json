{
  "name": "vuejs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --config ../../../eslint.config.cjs --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --config ../../../eslint.config.cjs --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@apollo/client": "^3.9.1",
    "@vue/apollo-composable": "^4.0.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@rushstack/eslint-patch": "^1.13.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.9.3",
    "vite": "^5.0.11",
    "vue-tsc": "^3.1.1"
  }
}
